{
	"name": "japtools",
	"version": "0.0.1",
	"scripts": {
		"dev": "esrun scripts/run.ts && vite dev --profile",
		"build": "esrun scripts/build_src.ts && vite build",
		"package": "esrun scripts/build_src.ts && svelte-kit package",
		"preview": "esrun scripts/run.ts && vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"dosetup": "esrun scripts/setup.ts"
	},
	"devDependencies": {
		"@prisma/client": "^4.2.1",
		"@sveltejs/adapter-auto": "1.0.0-next.66",
		"@sveltejs/kit": "1.0.0-next.444",
		"@types/bcrypt": "^5.0.0",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.35.1",
		"@typescript-eslint/parser": "^5.35.1",
		"bcrypt": "^5.0.1",
		"cookie": "^0.5.0",
		"cuid": "^2.1.8",
		"dotenv": "^16.0.1",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"functional-utilities": "^1.16.1",
		"node-sass": "^7.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.54.5",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.1",
		"svelte-preprocess": "^4.10.7",
		"ts-to-zod": "^1.13.1",
		"tslib": "^2.4.0",
		"type-fest": "^2.19.0",
		"typescript": "^4.8.2",
		"vite": "^3.0.9"
	},
	"type": "module",
	"dependencies": {
		"@digitak/esrun": "^3.2.10",
		"@macfja/svelte-persistent-store": "2.0.0",
		"@types/js-cookie": "^3.0.2",
		"@types/lodash-es": "^4.17.6",
		"@types/wanakana": "^4.0.3",
		"array-shuffle": "^3.0.0",
		"generate-password": "^1.7.0",
		"js-cookie": "^3.0.1",
		"lodash-es": "^4.17.21",
		"pkg-up": "^4.0.0",
		"wanakana": "^5.0.2",
		"zod": "^3.18.0"
	}
}
